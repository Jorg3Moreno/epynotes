{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
      "name": "FirebaseAPI",
      "urls": [
        "ADD_YOUR_FIREBASE_databaseURL_HERE"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "2h",
        "maxSize": 100,
        "timeout": "4s"
      }
    }
  ]
}
